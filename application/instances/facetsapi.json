{
  "metadata": {},
  "apiVersion": "v2",
  "kind": "service",
  "flavor": "k8s",
  "disabled": true,
  "spec": {
    "env": {
      "CONNECTION": "${redis.r1.interfaces.writer.connection_string}"
    },
    "loadbalancing": {
      "rules": [
        {
          "ingress": "ingressv12",
          "path": "/",
          "portName": "http",
          "domainPrefix": "fp1"
        },
        {
          "ingress": "ingressv1",
          "path": "/time",
          "portName": "http",
          "domainPrefix": "time"
        }
      ]
    },
    "permission": [],
    "release": {
      "strategy": "RollingUpdate",
      "image": "${blueprint.self.artifacts.poc-facetsapi}"
    },
    "runtime": {
      "size": {
        "value": "small",
        "namespace": "GP"
      },
      "autoscaling": {
        "cpuThreshold": "50",
        "max": 1,
        "min": 1
      },
      "ports": [
        {
          "name": "http",
          "port": 80
        }
      ]
    }
  }
}