{
    "disabled": true,
    "kind": "facets.modules.common.security",
    "apiVersion": "v1",
    "metadata": {
      "configValues": "https://github.com/falcosecurity/charts/tree/master/falco"
    },
    "spec": {
      "falco": {
        "config": {
          "falco": {
            "readinessProbe": {
              "initialDelaySeconds": 1800
            },
            "livenessProbe": {
              "initialDelaySeconds": 180123
            }
          },
          "resources": {
            "limits": {
              "cpu": "200m",
              "memory": "512Mi"
            },
            "requests": {
              "cpu": "100m",
              "memory": "100Mi"
            }
          }
        }
      }
    }
  }
