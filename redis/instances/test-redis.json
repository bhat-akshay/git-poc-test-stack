{
  "flavor": "k8s",
  "metadata": {
    "name": "redis-simple"
  },
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/redis/redis.schema.json",
  "advanced": {
    "k8s": {}
  },
  "kind": "redis",
  "disabled": false,
  "version": "0.1",
  "spec": {
    "authenticated": true,
    "redis_version": "6.2",
    "persistence_enabled": false,
    "size": {
      "writer": {
        "cpu": "1",
        "memory": "512M",
        "replica_count": 1
      },
      "reader": {
        "cpu": "1",
        "memory": "512M",
        "replica_count": 1
      }
    }
  },
  "out": {}
}